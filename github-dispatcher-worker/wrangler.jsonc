{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "github-dispatcher-worker",
	"main": "src/index.ts",
	"compatibility_date": "2025-06-18",
	"triggers": {
		"crons": [
			"*/1 * * * *"
		]
	},
	"vars": {
		"GITHUB_TOKEN": "${GITHUB_TOKEN}"
	},
	"observability": {
		"enabled": true
	}
}
